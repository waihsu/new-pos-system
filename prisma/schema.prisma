generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type OrdersCart {
  product_id String @db.ObjectId
  /// Multiple data types found: String: 84.6%, BigInt: 15.4% out of 13 sampled entries
  quantity   Json
}

model addon_categories {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  product_id String @db.ObjectId
}

model addons {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  addon_category_id String @db.ObjectId
  name              String
  price             String
}

model categories {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  location_id String @db.ObjectId
  name        String
}

model companies {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}

model customers {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  address String
  name    String
  phone   String
  user_id String @db.ObjectId
}

model locations {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  address   String
  asset_url String
  name      String @unique
  phone     String
  user_id   String @db.ObjectId
}

model locations_categories_products {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  category_id String @db.ObjectId
  location_id String @db.ObjectId
  product_id  String @db.ObjectId
}

model orders {
  id          String       @id @default(auto()) @map("_id") @db.ObjectId
  cart        OrdersCart[]
  location_id String       @db.ObjectId
  status      String
  /// Multiple data types found: Int: 10%, BigInt: 90% out of 10 sampled entries
  total       Json
  user_id     String       @db.ObjectId
}

model products {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  asset_url   String
  description String
  name        String
  price       String
  rating      String
  stock       String
}

model services {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  context     String
  location_id String @db.ObjectId
  title       String
}

model users {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  companies_id String? @db.ObjectId
  email        String  @unique
  role         String
  username     String
}
